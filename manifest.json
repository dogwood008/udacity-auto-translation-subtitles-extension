{
    "manifest_version": 2,
    "name": "Udacity auto translation subtitles extension",
    "description": "Udacity auto translation subtitles extension",
    "version": "0.1",
    "content_scripts": [{
        "matches": ["https://www.youtube.com/embed/*"],
        "js": ["jquery-3.1.1.min.js", "content_script.js"],
        "run_at": "document_end",
        "all_frames": true
    }],
    "permissions": [
      "activeTab"
    ]
}
